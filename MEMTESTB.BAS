1000 PN=&HFD86:AD=&H6000
1010 POKE &HFD94,&H80:POKE &HFD95,&H08
1020 DEF FNA$(A)=RIGHT$("000"+HEX$(A),4)
1030 DEF FND$(A)=RIGHT$("0"+HEX$(A),2)
1040 O=PEEK(PN)
1050 FOR I=&H40 TO &HBF
1060  POKE PN,I
1070  PRINT FND$(I);"000=";FND$(PEEK(AD))
1080  POKE AD,I
1090 NEXT I
1100 FOR I=&H40 TO &HBF
1110  POKE PN,I
1120  IF I MOD 16=0 THEN PRINT:PRINT FND$(I);"000";
1130  PRINT " ";FND$(PEEK(AD));
1140 NEXT I
1150 PRINT
1160 POKE PN,O
